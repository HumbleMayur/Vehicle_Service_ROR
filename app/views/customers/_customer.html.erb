<div id="<%= dom_id customer %>">
  <p>
    <strong>Name:</strong>
    <%= customer.name %>
    
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= customer.phone %>
  </p>

  <p>
    <strong>Dob:</strong>
    <%= customer.DOB %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= customer.address %>
  </p>

  <p>
    <strong>Vehicle type:</strong>
    <%= customer.vehicle_type %>
  </p>

  <p>
    <strong>Vehicle number:</strong>
    <%= customer.vehicle_number %>
  </p>

</div>
